<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gr.quixfer" version="1.0.0-beta" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Keyboard">
        <param name="android-package" value="com.ionic.keyboard.IonicKeyboard" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Focus">
        <param name="android-package" value="fr._46cl.focus.Focus" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <icon density="mdpi" src="www/img/quixfer_mdpi.png" />
    <icon density="hdpi" src="www/img/quixfer_hdpi.png" />
    <icon density="xhdpi" src="www/img/quixfer_xhdpi.png" />
    <icon density="xxhdpi" src="www/img/quixfer_xxhdpi.png" />
    <splash density="hdpi" src="www/img/quixfer_hdpi_splash.png" />
    <name>Quixfer</name>
    <description>
        Hybrid App for smart locally money management of Buxfer expenses and incomes 
    </description>
    <author email="gianluca.ragnoni@gmail.com" href="https://it.linkedin.com/in/gianlucaragnoni">
        Gianluca Ragnoni
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="AutoHideSplashScreen" value="false" />
</widget>
